					image_tracker='s';
				}
				else if(image_tracker=='s'){
					image.src="images/aas2.jpg";
					image_tracker='t';
				}
				else if(image_tracker=='t'){
					image.src="images/iitb.jpg";
					image_tracker='4';
				}
				else if(image_tracker=='4'){
					image.src="images/105.jpg";
					image_tracker='5';
				}
				else if(image_tracker=='5'){
					image.src="images/108.jpg";
					image_tracker='6';
				}
				else if(image_tracker=='6'){
					image.src="images/111.jpg";
					image_tracker='f';
				}

			}
			setInterval('change()',2000);
